(executables
 ((names (discover))
  (libraries (base stdio configurator))
  (preprocess (pps (ppx_jane ppx_driver.runner)))))

(rule
 ((targets (yaml-ccopt.sexp
            yaml-cclib.sexp
            yaml-ccopt
            yaml-cclib))
  (deps    (discover.exe))
  (action  (run ${<} -ocamlc ${OCAMLC}))))

(jbuild_version 1)
